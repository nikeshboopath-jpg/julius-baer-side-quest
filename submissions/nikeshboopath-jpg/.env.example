# Example .env - environment variables that override config.ini values
# Copy to .env and edit values for local development

# Base endpoint for the banking API (base URL, no trailing paths required)
TRANSFER_ENDPOINT=http://localhost:8123/

# Dry run mode (true/false)
DRY_RUN=true

# Network timeout in seconds
TIMEOUT=5.0

# Optional auth credentials (used only if you wire get_auth_token in main)
#AUTH_USERNAME=alice
#AUTH_PASSWORD=any
#AUTH_CLAIM=transfer
